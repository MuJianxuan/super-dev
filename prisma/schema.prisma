// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id String
  email String
  password_hash String
  name String
  created_at DateTime
  updated_at DateTime
}

model Auth_tokens {
  id String
  user_id String
  token String
  expires_at DateTime
  created_at DateTime
}
